@model CarRentalSystem.Models.SystemSettingModel
@{
    ViewData["Title"] = "Edit Setting";
}
<h2>Edit Setting</h2>
<form asp-action="Edit">
    <div class="mb-3">
        <label asp-for="Key" class="form-label"></label>
        <input asp-for="Key" class="form-control" readonly />
    </div>
    <div class="mb-3">
        <label asp-for="Value" class="form-label"></label>
        <input asp-for="Value" class="form-control" />
        <span asp-validation-for="Value" class="text-danger"></span>
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
    <a asp-action="Index" class="btn btn-secondary">Cancel</a>
</form>